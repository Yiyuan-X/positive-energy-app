{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "pnpm run build",
  "outputDirectory": "build",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
